{
  "name": "eslint-plugin-weblint-security",
  "homepage": "https://github.com/MarkKragerup/weblint-eslint-security",
  "version": "1.2.9",
  "description": "ESLint rules for enhanced security - even for React and Node.js!",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run unit-test",
    "unit-test": "mocha tests/rules/*.js",
    "dist-test": "npm run build && mocha dist/tests/rules/*.js",
    "ts-test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/rules/*.ts'",
    "build": "tsc"
  },
  "keywords": [
    "ESLint",
    "Security",
    "React",
    "Node.js",
    "Express",
    "Weblint"
  ],
  "author": "Mark Kragerup & Mathias HÃ¸yrup Nielsen",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-utils": "^2.1.0",
    "mocha": "^8.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.29.0",
    "@types/node": "^16.11.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
